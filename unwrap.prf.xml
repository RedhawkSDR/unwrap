<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="Val1" mode="readwrite" type="float">
    <description>Boundary for unwrapping.</description>
    <value>-3.1415926535897931</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="Val2" mode="readwrite" type="float">
    <description>Boundary for unwrapping.</description>
    <value>3.1415926535897931</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="cxOperator" mode="readwrite" type="string">
    <description>The operation to use to map complex values to real values so that they can be unwrapped.  For doing phase unwrapping - this should be set as &quot;arg&quot;</description>
    <value>phase</value>
    <enumerations>
      <enumeration label="phase" value="phase"/>
      <enumeration label="abs" value="abs"/>
      <enumeration label="real" value="real"/>
      <enumeration label="imag" value="imag"/>
      <enumeration label="norm" value="norm"/>
    </enumerations>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
